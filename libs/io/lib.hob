import def;
import internal;

public type Fd = i32;

extern fun Fd.write(self, ptr: *u8, size: def.Size);

public fun Fd.putStr(self, data: internal.Str) {
    self.write(data.raw, data.length);
}

public fun Fd.putChar(self, char: u8) {
    self.putStr(internal.string.new(&char, 1));
}

public const out: Fd = 1;
