import std;
import io;
import list;

use list.{ List, newFromSlice, free, appendSlice, push };

global fun main() -> i32 {
    var hi = newFromSlice.<u8>("hello");
    defer free.<u8>(&hi);

    appendSlice.<u8>(&hi, ", ");
    appendSlice.<u8>(&hi, "world!");
    push.<u8>(&hi, '\n');

    io.out.putString(hi.data as string);
    return 0;
}
