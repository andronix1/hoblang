local type usize = u32;

local type Slice<T> = struct {
    pointer: *T,
    length: usize
};

local extern(write) fun write(fd: i32, ptr: *u8, count: usize);

local fun print(slice: Slice.<u8>) {
    write(1, slice.pointer, slice.length);
}

local global fun main() -> i32 {
    print(<Slice.<u8>>{
        pointer: "super duper\n",
        length: 12
    });
    return 0;
}
