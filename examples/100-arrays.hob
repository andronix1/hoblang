import io;
import std;

global fun main() -> i32 {
    var e = <u8>['a', 'b', 'c', '\n'];

    io.out.putString("array to slice: ");
    io.out.putString(std.str.new(&e[0], 4));

    io.out.putString("one by one: ");
    var i: usize = 0;
    while i < e.length {
        io.out.putChar(e[i]);
        i += 1;
    }

    io.out.putString("putInt32: ");
    io.out.putInt32(0 - 1234);
    io.out.putString(", ");

    io.out.putInt32(1234);
    io.out.putString("\n");

    return 0;
}
